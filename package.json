{
  "name": "jupyter-js-services",
  "version": "0.19.0",
  "description": "Client APIs for the Jupyter services REST APIs",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@types/minimist": "^1.1.29",
    "@types/text-encoding": "0.0.30",
    "minimist": "^1.2.0",
    "path-posix": "^1.0.0",
    "phosphor": "^0.5.0",
    "requirejs": "^2.2.0",
    "url": "^0.11.0",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.42",
    "@types/ws": "0.0.33",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "requirejs": "^2.2.0",
    "rimraf": "^2.4.2",
    "text-encoding": "^0.5.2",
    "typedoc": "github:jasongrout/typedoc#ts2",
    "typescript": "^2.0.3",
    "webpack": "^1.13.1",
    "ws": "^1.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "clean": "rimraf docs && rimraf lib && rimraf test/build && rimraf test/coverage",
    "build:src": "tsc --project src",
    "build:test": "tsc --project test/src",
    "build": "npm run build:src && npm run build:test",
    "example:browser": "cd examples/browser && npm run update && npm run build",
    "example:node": "cd examples/node && npm install",
    "build:examples": "npm run example:browser && npm run example:node",
    "docs": "typedoc --mode file --module commonjs --excludeNotExported --target es5 --moduleResolution node --out docs/ src",
    "prepublish": "npm run build && webpack",
    "test:coverage": "istanbul cover --dir test/coverage _mocha -- test/build/test*.js \"test/build/**/*.spec.js\" --foo bar",
    "test:integration": "cd test && python integration_test.py",
    "test:debug": "mocha test/build/test*.js \"test/build/**/*.spec.js\" --foo bar --debug-brk",
    "test": "mocha test/build/test*.js \"test/build/**/*.spec.js\" --foo bar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/jupyter-js-services"
  },
  "keywords": [
    "jupyter",
    "services",
    "notebook"
  ],
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/*.js",
    "lib/*.d.ts",
    "dist/*.js",
    "dist/**/*.js"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-services/issues"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-services"
}
