{
  "name": "jupyter-js-services",
  "version": "0.5.5",
  "description": "Client APIs for the Jupyter services REST APIs",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "jupyter-js-utils": "^0.3",
    "phosphor-disposable": "^1.0.5",
    "phosphor-signaling": "^1.2.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "text-encoding": "^0.5.2",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2",
    "ws": "^1.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "peerDependencies": {
    "phosphor-disposable": "^1.0.5",
    "phosphor-signaling": "^1.2.0"
  },
  "scripts": {
    "clean": "rimraf docs && rimraf lib && rimraf test/build",
    "build:src": "tsc --project src",
    "build:test": "tsc --project test/src",
    "build": "npm run build:src && npm run build:test",
    "docs": "typedoc --options scripts/tdoptions.json",
    "prepublish": "npm run build",
    "test:coverage": "cd test && istanbul cover _mocha -- build/test*.js",
    "test:integration": "cd test && python integration_test.py",
    "test:debug": "mocha test/build/test*.js --debug-brk",
    "test": "mocha test/build/test*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/jupyter-js-services"
  },
  "keywords": [
    "jupyter",
    "services",
    "notebook"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-services/issues"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-services"
}
